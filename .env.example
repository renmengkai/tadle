# 代理设置 (true/false)
USE_PROXY=true

# 浏览器模式设置 (true=无头模式, false=有头模式)
HEADLESS_MODE=true

# 重试次数
MAX_RETRIES=3

# ====== 日志配置 ======
# 日志级别: error, warn, info, debug
# error - 只输出错误
# warn  - 错误 + 警告
# info  - 错误 + 警告 + 关键信息（默认）
# debug - 所有日志（包含调试信息）
LOG_LEVEL=info

# ====== 并发配置 ======
# Worker数量（多进程），默认等于代理数量，最大建议50
# 1000个账号 + 50个代理，建议设置为50
CONCURRENCY=50

# 每个Worker内部并发数（仅纯API模式有效）
# 建议1-3，太高可能触发频率限制
BATCH_CONCURRENCY=2

# 使用固定的浏览器指纹值（用于调试）
FIXED_BROWSER_VALUES=false

# Cloudflare Turnstile sitekey
# 从目标网站获取
TURNSTILE_SITEKEY=0x4AAAAAAAM8ceq5KhP1uJBt

# yesCaptcha API Key (用于解决 Turnstile 验证码)
# 获取地址: https://yescaptcha.com
# 配置此项和 TURNSTILE_SITEKEY 后将启用纯API模式（无需浏览器）
YESCAPTCHA_CLIENT_KEY=your_yescaptcha_client_key_here
